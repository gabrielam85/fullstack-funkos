<main class="create-container container">
  <h1 class="create-title">CREAR NUEVA CATEGORÍA</h1>
  <form class="create-form" action="/category/create" method="post">
    <section class="create-item">
      <label class="create-label" for="nombre">Nombre de la categoría:</label>
      <input
        class="create-text"
        type="text"
        name="nombre"
        id="nombre"
        value="<%= typeof values != 'undefined' ? values.nombre : '' %>"
        required
        placeholder="STAR WARS"
        minlength="3"
      />
    </section>
    <section class="create-actions">
      <input class="create-link" type="submit" value="Agregar Categoría" />
      <input class="create-link" type="reset" value="Limpiar" />
    </section>
  </form>
</main>
<%- contentFor('title') %> Creación Categoría - Funkoshop
